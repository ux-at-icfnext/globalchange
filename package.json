{
    "scripts": {
        "watch:sass": "sass --watch assets/css:_site/css",
        "build:sass": "sass assets/css:_site/css",
        "watch:eleventy": "npx @11ty/eleventy --serve",
        "build:eleventy": "npx @11ty/eleventy",
        "start": "npm-run-all build:sass --parallel watch:*",
        "build": "npm-run-all build:*"
    },
    "devDependencies": {
        "@11ty/eleventy": "^1.0.1",
        "npm-run-all": "^4.1.5",
        "sass": "^1.53.0"
    },
    "dependencies": {
        "@uswds/uswds": "^3.0.2"
    }
}
